<li id="image-<%= image.id  %>" class="image-background">
  <% if image.url != '' %>
    <img src="<%= image.url %>" >
  <% end %>
  <br>
  Image Cover: <%= image.cover %><br>
  P ID<%= image.paragraph_id %><br>
  Height: <%= image.height %><br>
  Width: <%= image.width %><br>
  display caption?
    <% if image.display_caption %>
      true
    <% else %>
      false
    <% end %><br>
  <% if image.display_caption %>caption: <%= image.try(:caption) %><br><% end %>
</li>
